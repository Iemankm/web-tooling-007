{
    "$schema": "http://json.schemastore.org/eslintrc",
    
    "extends": [
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "root": true,
    "env": {
        "browser": true,
        "es2021":true
    },
    "plugins": ["prettier"],
    "rules":{
        "prettier/prettier": [
            "error",
            {
                "endOfLine": "auto"
            }
        ]
    }
    
}